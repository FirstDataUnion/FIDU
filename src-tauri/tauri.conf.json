{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "FIDU ",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../../src/apps/acm-front-end/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm --prefix src/apps/acm-front-end run dev",
    "beforeBuildCommand": "npm --prefix src/apps/acm-front-end run build"
  },
  "app": {
    "windows": [
      {
        "title": "FIDU",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../src/fidu_core/front_end/static"
    ]
  }
}
